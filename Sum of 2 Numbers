#include <stdio.h>

int main() {
    // Declaring Variables as integer
    int num1, num2, sum;
    
    // Taking Input from user
    num1 = printf("Enter A Number: ");
    scanf("%d", &num1);
    
    num2 = printf("Enter Second Number: ");
    scanf("%d", &num2);
    
    // Calculating Sum
    sum = num1 + num2;
    
    // Printing Sum
    printf("%d", sum);
    
    
    
}
